package test;

public class OddOcurrance {

	public static void main(String[] args) {
		
		int arr[]= {12,23,34,12,12,23,12};
		
		for(int i=0;i<arr.length;i++) {
			int oddChk=arr[i];
			int cnt=0;
			for(int j=i;j<arr.length;j++) {
				int NextNum=arr[j];
				if(oddChk==NextNum) {
					cnt++;
				}
				
			}
			System.out.println(oddChk +": " +cnt);
			 oddChk(cnt);
		}

	}
	
	public static int oddChk(int i) {
		int result = i%2;
		if(result > 1) {
			return result;
		}
		return 0;
	}

}
